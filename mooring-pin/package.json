    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@pinia/nuxt": "^0.9.0",
    "lucide-vue-next": "^0.469.0",
    "nuxt": "^3.14.1592",
    "nuxt-mapbox": "^1.6.1",
    "pinia-plugin-persistedstate": "^4.2.0",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "@nuxtjs/tailwindcss": "^6.12.2",
    "@openapitools/openapi-generator-cli": "^2.15.3",
    "mapbox-gl": "^3.9.1"
